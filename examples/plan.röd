{
	import "planlib.rÃ¶d"
}

main args... {
	file := ""
	while [ #args > 0 ] do
		case(args[0]) | pull(itIs, somethingElse)
		if itIs("-h", "--help") do
			print "usage: plan options file"
			return
		done
		if itIs("-v", "--version") do
			print "plan 0.1"
			return
		done
		if somethingElse do
			file = args[0]
			break
		done
		shift args
	done
	
	error("file not specified") if [ file = "" ]
	
	execPlan(file, new map)
}
