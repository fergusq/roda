echo teksti {
	push teksti "\n"
}

duplicate {
	pull arvo
	push arvo
	push arvo	
}

numberOfArgs args... {
	echo #args
}

main {
	arvo -set "kissa"
	arvo | duplicate | while pull -r arvo; do
		echo arvo
	done
	seq 1 10 | while pull -r arvo; do
		echo arvo
	done
	("sakka" "banaani" "vakka") | grep ".akka" | replace "v" "p" | while pull -r arvo; do
		echo arvo
	done
	echo !(list "koe" "eok\\")[0]
	numberOfArgs 1 2 3 4
}
